  "scripts": {
    "start": "react-app-rewired start",
    "start:electron": "concurrently -k \"cross-env BROWSER=none react-app-rewired start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "sync": "react-app-rewired build && cap sync && cap update",
    "android": "npm run sync && npx cap run android",
    "build": "rimraf ./build && react-app-rewired build",
    "build:electron:mac": "react-app-rewired build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "build:electron:win": "react-app-rewired build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "build:electron:linux": "react-app-rewired build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
